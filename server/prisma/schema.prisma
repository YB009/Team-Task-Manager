datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          String   @id @default(cuid())
  email       String   @unique
  name        String?
  provider    String
  firebaseUid String?
  googleId    String?  @unique
  facebookId  String?  @unique
  githubId    String?  @unique
  twitterId   String?  @unique

  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
